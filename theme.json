{
    "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
    "settings": {
        "color": {
            "palette": [
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                },
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#0b4f6c"
                },
                {
                    "name": "Tint",
                    "slug": "tint",
                    "color": "#ecf8f8"
                }
            ],
            "custom": false,
            "customDuotone": false,
            "customGradient": false,
            "duotone": [],
            "gradients": [],
